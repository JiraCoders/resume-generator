{
  "name": "frontend",
  "displayName": "SwiftCV",
  "version": "2.1.2",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "lint": "eslint ./src",
    "format": "prettier --write ./src"
  },
  "dependencies": {
    "@mantine/charts": "^7.17.4",
    "@mantine/core": "^7.13.5",
    "@mantine/hooks": "^7.13.5",
    "@mantine/nprogress": "^7.13.5",
    "@swc/types": "^0.1.19",
    "plasmo": "^0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "2",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@tabler/icons-react": "^3.21.0",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "postcss": "^8.4.48",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.14",
    "typescript": "5.3.3",
    "typescript-eslint": "^8.13.0"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "contextMenus",
      "notifications",
      "tabs",
      "scripting",
      "activeTab",
      "sidePanel"
    ],
    "commands": {
      "generate-tailored-resume": {
        "suggested_key": {
          "default": "Alt+T"
        },
        "description": "Generate tailored resume from selected text"
      }
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "lmdb",
      "msgpackr-extract",
      "sharp"
    ]
  }
}